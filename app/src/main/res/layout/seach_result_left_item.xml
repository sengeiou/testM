<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/seach_ranking_linear"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    <TextView
        android:id="@+id/search_result_pop_left_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/padding_dp_15"
        android:paddingStart="@dimen/padding_dp_25"
        android:paddingTop="@dimen/padding_dp_15"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:text="content"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/text_size_14"
        />

    <ImageView
        android:id="@+id/search_result_pop_left_arrows"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginStart="@dimen/padding_dp_10"
        android:layout_gravity="center_vertical"
        android:src="@drawable/seach_select"
        android:visibility="gone"
        />
</RelativeLayout>
